#DAY 5 ASSIGNMENT
#1st program
list=[0,1,2,10,4,1,0,56,2,0,1,3,0,56,0,4]
c=list.count(0)
for i in range(1,c+1):
	list.remove(0)
list.sort()
for i in range(1,c+1):
	list.append(0)
print(list)
#2nd program
list1=[10,20,40,60,70,80]
list2=[5,15,25,35,45,60]
list3=list1+list2
#list3.sort()
#print(list3)
k=0
p=1
i=1
while(i<len(list3)):
	if(list3[k]>list3[i]):
		a=list3[k]
		list3[k]=list3[i]
		list3[i]=a
	i=i+1
	if(i==12):
		p=p+1
		i=p
		k=k+1
print(list3)
